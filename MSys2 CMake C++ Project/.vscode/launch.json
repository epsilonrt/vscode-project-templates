{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug CMake Project (MSYS2 ucrt64)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      // ...existing code...
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/Info/msys64/ucrt64/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}